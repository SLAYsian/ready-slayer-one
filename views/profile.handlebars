{{#each userOutcomes}}
  <div>
    <h4>{{this.name}}</h4>
    <p>{{this.description}}</p>
    <h5>Character:</h5>
    {{#if this.character}}
      <p>Name: {{this.character.name}}</p>
      <p>Avatar: <img src="{{this.character.avatar}}" alt="Character Avatar"></p>
    {{else}}
      <p >No character available.</p>

    {{/if}}
    <button class="delete-button" data-character-id="{{this.character_id}}" data-id="{{this.id}}">Delete</button>
  </div>
{{/each}}

<script src="/js/profile.js"></script>
